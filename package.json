{
  "name": "eslint-scrape-defaults",
  "version": "1.0.3",
  "description": "Generates an .eslintrc config file with the recommended default values and pretty comments.",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "eslint-config": "./bin/scrape-defaults.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "eslint": "^2.9.0",
    "expand-tilde": "^1.2.1",
    "json5": "^0.5.0",
    "lodash": "^4.12.0",
    "minimist": "^1.2.0",
    "word-wrap": "^1.1.0",
    "yakuza": "^1.0.7"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": false,
      "node": true
    },
    "globals": {
      "Promise": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": [
        2,
        "always"
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ]
    }
  }
}
